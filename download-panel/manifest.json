{
  "name": "Download panel",
  "version": "0.1.0",
  "description": "Adds a download panel to the Developer Tools.",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "devtools_page": "devtools.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "downloads",
    "clipboardRead",
    "storage"
  ],
  "minimum_chrome_version": "42",
  "manifest_version": 2
}
